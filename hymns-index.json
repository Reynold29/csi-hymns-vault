[
    {
      "hymn_number": 206,
      "title": "Aghoramanakleshavu",
      "signature": "Irregular",
      "lyrics": [
        [
          "Aghora Manakleshavu",
          "Sampraapta vaadarenu?",
          "Golaata Chinteyaadaroo",
          "Upadra Kottarenu?",
          "Ninna Bhaara Bhaktane",
          "Ellaa Lesu Maaduve",
          "Emba Divya Maatanne",
          "Nambu, Nambu, Nambu."
        ],
        [
          "Swamitra Shatruvaadaroo",
          "Anyaaya Hondidaagyoo",
          "Kangettu Taapapattaroo",
          "Nee Dharyavaagi Nambu",
          "Ninna Bhaara Bhaktane"
        ],
        [
          "Nee Melu Maaduttiralu",
          "Virodha Bandarenu?",
          "Yathaarthavaagi Baalalu",
          "Durnaamapattarenu?",
          "Ninna Bhaara Bhaktane"
        ],
        [
          "Bhooloka Bandhumitraru",
          "Hodaagyoo Dukha Beda",
          "Melloka Tande Endigoo",
          "Ninnannu Bidalaara",
          "Ninna Bhaara Bhaktane"
        ],
        [
          "Anaathanaagi Iddaroo",
          "Aayaasa Chinte Eke?",
          "Suloka dootarellaroo",
          "Ninnannu Kaayuttaare",
          "Ninna Bhaara Bhaktane"
        ],
        [
          "Bhoomyaasheyalla Hodaroo",
          "Samaadhi Maatra Eega",
          "Nin Munde Kaanabandaroo",
          "Apaayavenoo Illa",
          "Ninna Bhaara Bhaktane"
        ]
      ]
    },
    {
      "hymn_number": 175,
      "title": "Atyantakrupe Tumbida",
      "signature": "C M",
      "lyrics": [
        [
          "Atyanta Krupe Tumbida",
          "Dayaalu Pitanu",
          "Apaatranaada Papiya",
          "Vimochisidanu"
        ],
        [
          "Katinavaada Hrudaya",
          "Maarpadisidanu",
          "Apaaravaada Akrama",
          "Mannisibittanu"
        ],
        [
          "En Dukhavulla Aatmakke",
          "Santoshavittanu",
          "Swakeeyajanarondige",
          "Swatantra Kottanu"
        ],
        [
          "Nannannu preeti Maadida",
          "Dayaalu Pitane",
          "Naanendoo Ninna Naamava",
          "Kondaadikolluve"
        ]
      ]
    }
  ]
  